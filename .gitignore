# Czarina .gitignore

# Runtime status and logs
status/
*.log
nohup.out

# Development/testing artifacts
czarina-inbox/
czarina-development-archive/
archive/

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
env/
venv/
.venv

# OS
.DS_Store
Thumbs.db
*.swp
*.swo
*~

# IDE
.vscode/
.idea/
*.iml
.claude/

# Temp files
tmp/
temp/
*.tmp

